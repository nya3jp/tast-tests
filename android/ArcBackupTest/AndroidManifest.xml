    package="org.chromium.arc.testapp.backup">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:label="@string/app_name">
        <meta-data android:name="com.google.android.backup.api_key" android:value="AEdPqrEAAAAIGQ9OlqBeWyOzInRlDZM50lyoL-iBJyZAKjWQGA" />
        <activity android:name="org.chromium.arc.testapp.backup.BackupActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
