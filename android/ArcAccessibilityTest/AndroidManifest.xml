        android:targetSdkVersion="29" />
    <application
        android:label="@string/app_name">
        <activity android:name=".MainActivity" android:exported="true" android:label="@string/main_activity_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

