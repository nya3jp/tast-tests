  {
    "key": "",
    "name": "DLP extension to get clipboard data",
    "description": "For testing the DLP clipboard restriction",
    "manifest_version": 2,
    "version": "0.1",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "commands": {
        "undo": {
            "suggested_key": {
                "default": "Ctrl+Z"
            },
            "description": "A test command which doesn't affect clipboard content"
        }
    },
    "permissions": [
        "clipboardRead"
    ]
  }