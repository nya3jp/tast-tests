;
; 4k random reads over a 1 GiB area with iodepth=4.
;

[4k_read_qd4]
filename=${FILENAME}
size=${FILESIZE}

time_based
runtime=4m

ioengine=libaio
iodepth=4
direct=1

readwrite=randread

bs=4k
norandommap